<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp9.0</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <!-- Link the gcirtlobj.obj file at compile time using GEMSTONE environment variable -->
    <NativeLibrary Include="$(GEMSTONE)\lib\gcirtlobj.obj" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="YamlDotNet" Version="16.3.0" />
  </ItemGroup>
  
</Project>
