	<PropertyGroup Label="InteropCompilation">
		<EmitCompilerGeneratedFiles>true</EmitCompilerGeneratedFiles>
		<!-- Pick one. Not both. :) -->
		<!--<DefineConstants>$(DefineConstants);LEGACY;</DefineConstants>-->
		<DefineConstants>$(DefineConstants);THREADSAFE;</DefineConstants>
	</PropertyGroup>

	<ItemGroup Condition="'$(DefineConstants.Contains(`;LEGACY;`))'" Label="InteropCompilation">
		<None Include="..\CCKDLLS\LooseDLLs\RT.dll">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</None>
	</ItemGroup>

	<PropertyGroup Condition="'$(DefineConstants.Contains(`;THREADSAFE;`))'" Label="InteropCompilation">
		<DefaultItemExcludes>$(DefaultItemExcludes);Legacy/*.*;Interop/RT.cs;</DefaultItemExcludes>
		<Visible>false</Visible>
	</PropertyGroup>

	<ItemGroup Condition="'$(DefineConstants.Contains(`;THREADSAFE;`))'" Label="InteropCompilation">
		<None Include="..\CCKDLLS\GemBuilderC_3.7.4.3\libgcits-3.7.4.3-64.dll">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
			<Visible>false</Visible>
		</None>
		<None Include="..\CCKDLLS\GemBuilderC_3.7.4.3\libssl-3.7.4.3-64.dll">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
			<Visible>false</Visible>
		</None>
		<None Include="..\CCKDLLS\GemBuilderC_3.7.4.3\msvcr100.dll">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
			<Visible>false</Visible>
		</None>
	</ItemGroup>

	<PropertyGroup Label="DefaultWarningSuppression">
		<NoWarn>$(NoWarn);CS1591;IDE1006</NoWarn>
	</PropertyGroup>
	<!-- After .NET 5 some globalization had breaking changes this reverts them
	See https://learn.microsoft.com/en-us/dotnet/core/extensions/globalization-icu -->
	<ItemGroup>
		<RuntimeHostConfigurationOption Include="System.Globalization.UseNls" Value="true" />
	</ItemGroup>
